<form method="post" action="{{ path('back_inscription_delete', {'id': inscription.id}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ inscription.id) }}">
    <button class="btn btn-danger waves-effect waves-light" style="width: 100%;">Supprimer</button>
</form>
